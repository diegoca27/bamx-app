{
  "expo": {
    "name": "bamx-app",
    "slug": "bamx-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "build": {
      "android": {
        "buildType": "apk"
      }
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundlerIdentifier": "com.diegoca_04.bamxapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.diegoca_04.bamxapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "51cf0589-fdcd-4005-b5fe-dd6c61440b32"
      }
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Permite que $(PRODUCT_NAME) use tu ubicación para mostrarte donaciones cercanas.",
          "locationWhenInUsePermission": "Permite que $(PRODUCT_NAME) use tu ubicación mientras la app está en uso para mostrarte donaciones cercanas." 
        }
      ]
    ]
  }
}